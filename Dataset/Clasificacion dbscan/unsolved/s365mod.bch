variables

x1 in [0.5, 100000000];
x2 in [0.5, 100000000];
x3 in [-100000000, 100000000];
x4 in [-100000000, 100000000];
x5 in [1, 100000000];
x6 in [-100000000, 100000000];
x7 in [1, 100000000];

minimize (x1*x2);

constraints

(((x5-x7)^2+(x4-x6)^2)-4)>=0;

((((x2*x4)-(x3*x5))/(x2^2+sqrt(x3^2)))-1)>=0;

((((x2*x6)-(x3*x7))/(x2^2+sqrt(x3^2)))-1)>=0;

((((((x3-x1)*x5)+(x1*x2))-(x2*x4))/((x3-x1)^2+sqrt(x2^2)))-1)>=0;

((((((x3-x1)*x7)+(x1*x2))-(x2*x6))/((x3-x1)^2+sqrt(x2^2)))-1)>=0;
end
